<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_start.xml"
    app:startDestination="@id/starterFragment">

    <fragment
        android:id="@+id/starterFragment"
        android:name="com.utotech.danzhehplus.fragments.StarterPages.StarterFragment"
        android:label="fragment_starter"
        tools:layout="@layout/fragment_starter" >
        <action
            android:id="@+id/action_starterFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_starterFragment_to_sighnUpFragment"
            app:destination="@id/sighnUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.utotech.danzhehplus.fragments.StarterPages.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_forgotPassFragment"
            app:destination="@id/forgotPassFragment" />
        <action
            android:id="@+id/action_loginFragment_to_sighnUpFragment"
            app:destination="@id/sighnUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/forgotPassFragment"
        android:name="com.utotech.danzhehplus.fragments.StarterPages.ForgotPassFragment"
        android:label="fragment_forgot_pass"
        tools:layout="@layout/fragment_forgot_pass" />
    <fragment
        android:id="@+id/sighnUpFragment"
        android:name="com.utotech.danzhehplus.fragments.StarterPages.SighnUpFragment"
        android:label="fragment_sighn_up"
        tools:layout="@layout/fragment_sighn_up" />
</navigation>